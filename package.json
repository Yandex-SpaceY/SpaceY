{
  "name": "space-y",
  "version": "1.0.0",
  "description": "Space Y game",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --mode development --open --hot",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "lint": "eslint . --fix --ext .ts,.tsx,.js && stylelint --fix **/*.scss",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yandex-SpaceY/SpaceY"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Yandex-SpaceY/SpaceY#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "cross-env": "7.0.3",
    "css-loader": "5.2.6",
    "eslint": "7.28.0",
    "express": "4.17.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "path": "0.12.7",
    "prettier": "2.3.1",
    "sass": "1.34.1",
