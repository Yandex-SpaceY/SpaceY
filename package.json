{
  "name": "space-y",
  "version": "1.0.0",
  "description": "Space Y game",
  "main": "server/index.ts",
  "scripts": {
    "clean": "rimraf dist",
    "start:webpack": "cross-env NODE_ENV=development webpack --mode=development --watch",
    "start:server": "wait-on dist/server.js && node dist/server.js",
    "start": "npm run clean && npm-run-all --print-label --parallel start:*",
    "build": "npm run clean  && cross-env NODE_ENV=production && webpack --mode production",
    "watch:server": "nodemon ./dist/server.js --watch ./dist",
    "watch:build": "cross-env NODE_ENV=development && webpack --mode development --watch",
    "watch:start": "npm-run-all --parallel watch:build watch:server",
    "lint": "eslint . --fix --ext .ts,.tsx,.js && stylelint --fix **/*.scss",
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yandex-SpaceY/SpaceY"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Yandex-SpaceY/SpaceY#readme",
